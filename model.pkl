import pandas as pd
from sklearn.ensemble import RandomForestClassifier
import pickle

# Laad je data
df = pd.read_csv('jouw_data.csv')

# Kenmerken en doel
X = df[['leeftijd', 'inkomen']]  # ← pas aan met jouw kolommen
y = df['klant_blijft']           # ← pas aan met jouw doelvariabele

# Train een model
model = RandomForestClassifier()
model.fit(X, y)

# Sla het model op
with open('model.pkl', 'wb') as f:
    pickle.dump(model, f)
